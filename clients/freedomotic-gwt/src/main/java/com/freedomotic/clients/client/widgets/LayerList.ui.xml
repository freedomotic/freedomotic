<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:cell="urn:import:com.google.gwt.user.cellview.client">

<ui:style>
    .cellStyle{
        border: 1px solid;
        padding: 3px;
        width: 200px;

    }

    .image{
        width: 100px;
        height: 150px;
    }

    .anchor{
        color: blue;
        text-decoration:underline;
    }
    .title{
        font-size: 14pt;
    }
</ui:style>
    <g:HTMLPanel>
        <g:FlowPanel>
            <g:Label addStyleNames="{style.title}">Environment Floors</g:Label>
            <cell:CellList ui:field='layerList' addStyleNames='{style.cellStyle}'></cell:CellList>
        </g:FlowPanel>
    </g:HTMLPanel>
</ui:UiBinder>