<command>
    <name>Turn off relay on Devantech Eth-Rly board</name>
    <receiver>app.actuators.protocol.devantech-eth-rly.in</receiver>
    <description>turn off the relay linked to this object on Devantech Eth-Rly board</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
             <property name="address" value="@owner.object.address"/>
        </properties>
    </properties>
</command>