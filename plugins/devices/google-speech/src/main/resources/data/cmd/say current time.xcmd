<com.freedomotic.reactions.Command>
    <name>Say current time</name>
    <receiver>app.actuators.media.google-speech.in</receiver>
    <description>say current time</description>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="say" value="It's @event.time.hour and @event.time.minute minutes"/>
        </properties>
    </properties>
</com.freedomotic.reactions.Command>
