<command>
    <name>Pulse the relay on Ipx800 board</name>
    <receiver>app.actuators.protocol.ipx800.in</receiver>
    <description>toggles for 1 second</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="command" value="SEND-PULSE-RELAY"/>
            <property name="address" value="@owner.object.address"/>
	</properties>
    </properties>
</command>