<command>
    <name>Turn OFF Relay on ProgettiHwSw Eth board</name>
    <receiver>app.actuators.protocol.phwswethv2.in</receiver>
    <description>turns off the relay linked to this object on ProgettiHwSw Eth board</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="command" value="CHANGE-STATE-RELAY"/>
            <property name="behavior" value="0"/>
	    <property name="address" value="@owner.object.address"/>
        </properties>
    </properties>
</command>
