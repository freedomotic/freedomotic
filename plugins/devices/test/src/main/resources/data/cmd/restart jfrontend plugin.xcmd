<com.freedomotic.reactions.Command>
    <name>Restart Jfrontend Plugin</name>
    <receiver>app.actuators.plugins.controller.in</receiver>
    <description>Restarts Java Desktop plugin</description>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="plugin" value="Desktop Frontend"/>
            <property name="action" value="restart"/>
        </properties>
    </properties>
</com.freedomotic.reactions.Command>
