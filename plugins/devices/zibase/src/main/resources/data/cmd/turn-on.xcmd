<command>
    <name>Turn on Zibase device</name>
    <receiver>app.actuators.protocol.zibase.in</receiver>
    <description>turns on the object linked to this zibase gateway</description>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="control" value="ON"/>
            <property name="address" value="@owner.object.address"/>
        </properties>
    </properties>
</command>