<trigger>
    <name>ZWay reads electric voltage</name>
    <description>ZWay reads electric voltage</description>
    <channel>app.event.sensor.protocol.read.zway</channel>
    <payload>
        <payload>
            <statement>
                <logical>AND</logical>
                <attribute>@event.object.type</attribute>
                <operand>EQUALS</operand>
                <value>electric-voltage</value>
            </statement>
            <statement>
                <logical>SET</logical>
                <attribute>behaviorValue</attribute>
                <operand>EQUALS</operand>
                <value>= behaviorValue=Math.round(@event.read.value*10)</value>
            </statement>
        </payload>
    </payload>
    <hardwareLevel>true</hardwareLevel>
    <delay>0</delay>
    <priority>0</priority>
    <maxExecutions>-1</maxExecutions>
    <numberOfExecutions>0</numberOfExecutions>
    <suspensionTime>0</suspensionTime>
</trigger>
