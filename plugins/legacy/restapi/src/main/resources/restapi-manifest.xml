<config>
    <properties>
        <property name="name"                   value="restapi"/>
        <property name="description"            value="Plugin for rest"/>
        <property name="version"                value="1.0.0"/>
        <property name="required"               value="5.2.0"/>
        <property name="category"               value="Api"/>
        <property name="short-name"             value="RestApi"/>
        <property name="startup-time"           value="on load"/>
        <property name="SSL_PORT"               value="8113"/>
        <property name="KEYSTORE_FILE"          value="keystore"/>
        <property name="KEYSTORE_TYPE"          value="JKS"/>
        <property name="KEYSTORE_PASWORD"       value="password"/>
        <property name="permissions"            value="*"/>
        <property name="Access-Control-Allow-Origin" value="*"/>
        <property name="Access-Control-Allow-Methods" value="GET,POST,DELETE,OPTIONS"/>
        <property name="Access-Control-Allow-Headers" 
                  value="Accept,Accept-Version,Authorization,Content-Length,Content-MD5,Content-Type,Date,Origin,X-Access-Token,X-Api-Version,X-CSRF-Token,X-File-Name,X-Requested-With"/>
        
    </properties>
</config>

                